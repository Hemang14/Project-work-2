									<tr>
										<th>ID</th>
										<th>TITLE</th>
										<th>CATEGORY</th>
										<th>RATING</th>
									</tr>
								</thead>
								<tbody>
									<%
					        if(movieData.length!=0){
					        var i=1;
					        movieData.forEach(function(data){
					        %>
									<tr>
										<td>
											<div class="main__table-text"><%=i; %></div>
										</td>
										<td>
											<div class="main__table-text"><a href="#"><%=data.movie_show_name %></a></div>
										</td>
										<td>
											<div class="main__table-text"><%=data.movie_show_indicator %></div>
										</td>
										<td>
											<div class="main__table-text main__table-text--rate"><i>	<img src="images/star.png" width="30px" alt=""></i><%=data.rating %></div>
										</td>
									</tr>
									<%  i++; }) %>
					        <% } else{ %>
					            <tr>
					                <td colspan="7">No Data Found</td>
					            </tr>
					        <% } %>
								</tbody>
							</table>
						</div>
					</div>
