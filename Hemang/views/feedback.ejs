<h2>FEED BACK FORM</h2>
<div style="background-color:#7c4789;">
</div>
<div class="container">
  <form action="/feedback" method="POST">
    <div class="row">
      <div class="col-25">
        <label for="username">User Name</label>
      </div>
      <div class="col-75">
        <input type="text" id="username" name="username" placeholder="Your name..">
      </div>
    </div>

    <div class="row">
        <div class="col-25">
          <label for="email">Mail Id</label>
        </div>
        <div class="col-75">
          <input type="email" id="email" name="email" placeholder="Your mail id..">
        </div>
      </div>
    <div class="row">
      <div class="col-25">
        <label for="country">Country</label>
      </div>
      <div class="col-75">
        <select id="country" name="country">
            <option value="none">Select Country</option>
          <option name="australia" value="australia">Australia</option>
          <option name="canada" value="canada">Canada</option>
          <option name="usa" value="usa">USA</option>
          <option name="russia" value="russia">Russia</option>
          <option name="japan" value="japan">Japan</option>
          <option name="india" value="india">India</option>
          <option name="china" value="china">China</option>
        </select>
      </div>
    </div>
    <div class="row">
      <div class="col-25">
        <label for="Video Clarity ">Video Clearity</label>
      </div>
      <div class="col-75">
        <select id="feedback" name="video_clarity">
            <option value="none">Select option</option>
          <option value="good">good</option>
          <option value="worst">worst</option>
          <option value="can be better">can be better</option>
           <option value="best">best</option>
          </select>
      </div>
    </div>
 <div class="row">
      <div class="col-25">
        <label for="feedback category 2 ">Video Content</label>
      </div>
      <div class="col-75">
        <select id="feedback" name="video_content">
            <option value="none">Select option</option>
          <option value="good">good</option>
          <option value="worst">worst</option>
          <option value="can be better">can be better</option>
           <option value="best">best</option>
          </select>
      </div>
    </div>
 <div class="row">
      <div class="col-25">
        <label for="feedback category 3 ">Room Communication and Chat</label>
      </div>
      <div class="col-75">
        <select id="feedback" name="communication">
